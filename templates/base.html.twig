        {{ encore_entry_link_tags('app') }}
        {% block stylesheets %}{% endblock %}
        <script data-ad-client="ca-pub-3511968554265090" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
